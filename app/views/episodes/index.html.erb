
<table id="episode-list">
<%- groupify(collection, Proc.new {|ep| ep.aired_at || ep.phile.file_created_at }).each do |pair|
    (group, episodes) = pair
-%>
<tr class="group"><td colspan="5">Aired less than <%= time_ago_in_words group %> ago</td></tr>
<%-
    episodes.each do |episode|
-%>
          <%= render episode %>
      <%- end -%>
  <%- end -%>
</table>
